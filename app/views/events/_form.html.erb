<%= form_with(model: @event, local: true) do |form| %>


<div class="event_content">
  <%= form.label :内容 %>
  <%= form.text_field :content %>
</div>

<div class="event_start">
  <%= form.label :開始日 %>
  <%= form.date_select :start_date %>
</div>

<div class="event_end">
  <%= form.label :終了日 %>
  <%= form.date_select :end_date %>
</div>

<div class="actions">
  <%= form.submit %>
</div>
<% end %>

<%= link_to '一覧', events_path %>
